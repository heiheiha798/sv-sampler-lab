{
    // 使用 IntelliSense 了解相关属性。 
    // 悬停以查看现有属性的描述。
    // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "启动程序",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}"
        },

        {
            "name": "merge-results", // 给这个配置起个名字
            "type": "cppdbg", // 使用 C/C++ 调试器
            "request": "launch",
            "program": "${workspaceFolder}/build/MySolver", // 可执行文件的路径
            "args": [ // 传递给 MySolver 的参数
                "merge-results",
                "${workspaceFolder}/my_run_output/manifest.json",
                "${workspaceFolder}/my_run_output/debug_final_result_vscode.json", // 新的输出文件名
                "5",
                "123"
            ],
            "stopAtEntry": false, // 如果设为 true, 会在 main 函数入口处停止
            "cwd": "${workspaceFolder}", // 当前工作目录设置为项目根目录
            "environment": [],
            "externalConsole": false, // true 会在新终端窗口运行程序，false 在VS Code调试控制台
            "MIMode": "gdb", // 使用 gdb
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "CMake: build" // 可选：在启动调试前自动构建项目 (需要配置 tasks.json)
        }
    ]     
        
    
}